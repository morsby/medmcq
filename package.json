{
  "name": "medmcq",
  "version": "2.0.0",
  "description": "En webapp til visning af MCQ-opgaver fra kandidaten i medicin pÃ¥ AU",
  "main": "server/server.js",
  "repository": "https://github.com/Morsby/medmcq",
  "scripts": {
    "start": "ts-node -P server/tsconfig.json -r tsconfig-paths/register build/server/server.js",
    "server": "cd server && nodemon",
    "client": "react-scripts start",
    "dev": "cross-env npm-run-all --parallel server client",
    "build": "npm-run-all --sequential build:client build:server",
    "build:client": "react-scripts build",
    "build:server": "cd server && tsc -p tsconfig.json",
    "test": "start-server-and-test testdev http://localhost:3000 cy:run",
    "test:ci": "start-server-and-test start http://localhost:3001 cy:run",
    "testdev": "cross-env NODE_ENV=\"test\" npm run dev",
    "postbuild": "cd server && knex migrate:latest",
    "generate": "graphql-codegen",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "db:reset": "cd server && cross-env NODE_ENV=\"test\" knex migrate:latest && cross-env NODE_ENV=\"test\" knex seed:run"
  },
  "proxy": "http://localhost:3001",
  "author": "Morsby & Thomas",
  "license": "MIT",
